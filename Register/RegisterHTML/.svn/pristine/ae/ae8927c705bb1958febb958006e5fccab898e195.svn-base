
<!doctype html public "-//w3c//dtd xhtml 1.0 transitional//en" "http://www.w3.org/tr/xhtml1/dtd/xhtml1-transitional.dtd"> 
<html xmlns="http://www.w3.org/1999/xhtml" > 
<head runat="server"> 
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<title>jquery给控件赋值 www.111cn.net</title> 
<script type="text/网页特效" src="网页特效/jquery-1.3.2.min.js"></script> 
<script type="text/javascript"> 
	var xml="123";
function loadXMLDoc(){
	var xmlhttp;

	alert("1:"+xml);
	if(window.XMLHttpRequest){
			xmlhttp=new XMLHttpRequest();
		}
	else{
			xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
		}

xmlhttp.onreadystatechange=function(){
		if(xmlhttp.readyState==4 &&xmlhttp.status==200)
			{
				document.getElementById("myDiv").innerHTML=xmlhttp.responseText;	
				xml="321";
			}
	}
	xmlhttp.open("get","modeling/SimpleRegister/Shu_Insitu_Temperature_S-1.xml",true);
	xmlhttp.send();
	window.alert('2:'+xml);
	}
function click_(){
	window.alert('3:'+xml);
	}
</script> 
</head> 
<body> 
 	<div id="myDiv"><h2>Let AJAX change this text</h2></div>
    <button type="button" onClick="loadXMLDoc()">改变内容</button>
    <button type="button" onClick="click_();">123</button>
</body> 
</html>